{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/dist",
      "files": [ "qrcode.min.js" ]
    },
    {
      "library": "twitter-bootstrap@4.6.0",
      "destination": "wwwroot/lib/bootstrap/dist/",
      "files": [
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/dist",
      "files": ["jquery.min.js"]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jquery-validation/dist",
      "files": ["jquery.validate.min.js"]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/dist",
      "files": ["jquery.validate.unobtrusive.min.js"]
    }
  ]
}